{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.0",
        "ext-mysqli": "*",
        "abraham/twitteroauth": "^3.1",
        "activecampaign/api-php": "^2.0",
        "amrshawky/laravel-currency": "^5.0",
        "atymic/twitter": "^3.1",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-dompdf": "^1.0",
        "barryvdh/laravel-snappy": "^1.0",
        "doctrine/dbal": "^3.5",
        "emilianotisato/nova-tinymce": "^1.2",
        "facebook/php-business-sdk": "^12.0",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.12.1",
        "google/apiclient-services": "^0.279.0",
        "googleads/google-ads-php": "^15.0",
        "googleads/googleads-php-lib": "^10.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "guzzlehttp/psr7": "^1.2",
        "intervention/image": "^2.7",
        "jorenvanhocht/laravel-share": "^4.2",
        "kraftbit/nova-tinymce5-editor": "^1.0",
        "laravel/cashier": "^13.8",
        "laravel/framework": "^9.0",
        "laravel/nova": "~3.0",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "mailchimp/marketing": "^3.0",
        "manogi/nova-tiptap": "^2.7",
        "sentry/sentry-laravel": "^3.1",
        "sloveniangooner/locale-anywhere": "dev-master",
        "socialiteproviders/apple": "^5.0",
        "srmklive/paypal": "^3.0",
        "stevebauman/location": "^6.2",
        "stripe/stripe-php": "^7.114",
        "symfony/intl": "^6.0",
        "symfony/mailer": "^6.0",
        "torann/geoip": "^3.0",
        "tuqqu/gender-detector": "^0.3.0",
        "twilio/sdk": "^6.43",
        "unisharp/laravel-filemanager": "dev-master",
        "ynacorp/nova-swatches": "^1.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php",
            "config/constants.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
